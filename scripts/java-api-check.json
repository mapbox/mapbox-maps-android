[
  {
    "extension": "revapi.java",
    "configuration": {
      "checks": {
        "nonPublicPartOfAPI": {
          "reportUnchanged": false
        }
      },
      "filter": {
        "classes": {
          "regex": true,
          "include": ["com\\.mapbox\\.maps\\..*"]
        },
        "packages": {
          "regex": true,
          "include": ["com\\.mapbox\\.maps\\..*"]
        }
      }
    }
  }
]
